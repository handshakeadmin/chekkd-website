{
  "buildCommand": null,
  "outputDirectory": "out",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.chekkd.com" }],
      "destination": "https://chekkd.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/contact", "destination": "/contact/index.html" },
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
